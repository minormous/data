{
    "name": "minormous/data",
    "description": "Interfaces and traits for creating a data layer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Minormous Contributors",
            "homepage": "https://github.com/minormous"
        }
    ],
    "replace": {
        "sparkphp/data": "self.version"
    },
    "require": {
        "php": ">=8.3",
        "nesbot/carbon": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Minormous\\Data\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MinormousTests\\Data\\": "tests/"
        }
    }
}
